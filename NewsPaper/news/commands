Author.objects.create(user=user2)
Category.objects.create(category='Спорт')
Category.objects.create(category='Политика')
Category.objects.create(category='Наука и техника')
Category.objects.create(category='Кино')
A1=Author.objects.get(user=user1)
N=Post.objects.create(author=A1, typ='NW', title='qwe', text='qwe qwe qwe')
C=Category.objects.get(category='Спорт')
N.category.add(C)
C=Category.objects.get(category='Наука и техника')
N.category.add(C)
Ar=Post.objects.create(author=A1, typ='AR', title='qwe', text='qwe qwe qwe')
Ar.category.add(C)
Ar=Post.objects.create(author=A1, typ='AR', title='123test', text='texttexttetxttext123123123123123123123')
Ar.category.add(C)
K1=Comment.objects.create(user=user1, text='qwe qwe qwe', post=Ar)
K2=Comment.objects.create(user=user1, text='qwqwqw', post=Ar)
K3=Comment.objects.create(user=user1, text='qwqwqw', post=N)
K4=Comment.objects.create(user=user1, text='123test321', post=N)
K1.like()
